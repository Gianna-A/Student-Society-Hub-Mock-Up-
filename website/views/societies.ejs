<!DOCTYPE html>
<html lang="en">

<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link rel="stylesheet" href="/main.css">
	<title>Societies</title>
</head>

<body>

	<div class="content">
		<h1> Societies </h1>
		<ul>
			<li><a class="active" href="/">Home</a></li>
			<li><a href="/students">All Students</a></li>
			<li><a href="/societies">All Societies</a></li>
			<li><a href="/events">Events</a></li>
		  </ul>

		<section class="dataTable">
			<table class="data">
				<tr>
					<th>Name</th>
					<th>Email</th>
					<th> Meeting Time </th>
					<th> Meeting Day </th>
					<th> Actions </th>
				</tr>
				<tr>
				    <% societies.forEach(society=> { %>
				<tr>
						<td><%- society.Soc_Name %></td>
						<td><%- society.Soc_Email %></td>
						<td><%- society.Soc_Meeting_Time %></td>
						<td><%- society.Soc_Meeting_Day %></td>
						<td><a href="/societies/committeemem/<%- society.Soc_ID %>"> Committee Members </a>
						</tr>
						<% }) %>
				</tr>





			</table>
		</section>
	</div>

</body>

</html>